{
    "name": "lightsaml/symfony-bridge-idp",
    "license": "GPL-3.0+",
    "type": "library",
    "description": "Light SAML2 Symfony bridge for IDP bundle",
    "autoload": {
        "psr-0": {
            "LightSaml\\SymfonyBridgeIdpBundle\\Tests\\": "tests/",
            "LightSaml\\SymfonyBridgeIdpBundle\\": "src/"
        }
    },
    "require": {
        "php": "^7.2|^8.0",
        "lightsaml2/lightsaml": "^2.0",
        "lightsaml2/symfony-bridge": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../lightSAML"
        },
        {
            "type": "path",
            "url": "../SymfonyBridgeBundle"
        }
    ]
}
